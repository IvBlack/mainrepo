<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>{{ title }}</title>
    <link rel="stylesheet" href="css/style.css">
</head>
<body>
<div class="gallery">
    {% for img in images %}
        <a class="gallery-item" href="/image.php?id={{ img.id }}">
            <img src="{{ img.url }}" alt="{{ img.title }}">
        </a>
    {% endfor %}
</div>
</body>
</html>